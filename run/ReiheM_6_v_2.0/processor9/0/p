FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    front_top
    {
        type            wedge;
    }
    front_middle
    {
        type            wedge;
    }
    front_bottom
    {
        type            wedge;
    }
    back_top
    {
        type            wedge;
    }
    back_middle
    {
        type            wedge;
    }
    back_bottom
    {
        type            wedge;
    }
    axis_top
    {
        type            symmetryPlane;
    }
    axis_middle
    {
        type            symmetryPlane;
    }
    axis_bottom
    {
        type            symmetryPlane;
    }
    wall_top
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 100000;
    }
    wall_middle
    {
        type            fixedFluxPressure;
        gradient        nonuniform 0();
        value           nonuniform 0();
    }
    wall_bottom
    {
        type            fixedFluxPressure;
        gradient        nonuniform 0();
        value           nonuniform 0();
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    procBoundary9to8
    {
        type            processor;
        value           uniform 100000;
    }
    procBoundary9to10
    {
        type            processor;
        value           uniform 100000;
    }
}


