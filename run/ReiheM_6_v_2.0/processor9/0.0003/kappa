FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.0003";
    object      kappa;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -3 -1 0 0 0];

internalField   uniform 0.2421615142;

boundaryField
{
    front_top
    {
        type            wedge;
    }
    front_middle
    {
        type            wedge;
    }
    front_bottom
    {
        type            wedge;
    }
    back_top
    {
        type            wedge;
    }
    back_middle
    {
        type            wedge;
    }
    back_bottom
    {
        type            wedge;
    }
    axis_top
    {
        type            symmetryPlane;
    }
    axis_middle
    {
        type            symmetryPlane;
    }
    axis_bottom
    {
        type            symmetryPlane;
    }
    wall_top
    {
        type            calculated;
        value           uniform 0.2421615142;
    }
    wall_middle
    {
        type            calculated;
        value           nonuniform 0();
    }
    wall_bottom
    {
        type            calculated;
        value           nonuniform 0();
    }
    inlet
    {
        type            calculated;
        value           nonuniform 0();
    }
    outlet
    {
        type            calculated;
        value           nonuniform 0();
    }
    procBoundary9to8
    {
        type            processor;
        value           uniform 0.2421615142;
    }
    procBoundary9to10
    {
        type            processor;
        value           uniform 0.2421615142;
    }
}


