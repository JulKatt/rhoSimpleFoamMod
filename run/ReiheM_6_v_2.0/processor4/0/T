FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 405.65;

boundaryField
{
    front_top
    {
        type            wedge;
    }
    front_middle
    {
        type            wedge;
    }
    front_bottom
    {
        type            wedge;
    }
    back_top
    {
        type            wedge;
    }
    back_middle
    {
        type            wedge;
    }
    back_bottom
    {
        type            wedge;
    }
    axis_top
    {
        type            symmetryPlane;
    }
    axis_middle
    {
        type            symmetryPlane;
    }
    axis_bottom
    {
        type            symmetryPlane;
    }
    wall_top
    {
        type            convectiveRadiativeHeatFlux;
        mode            convectiveRadiative;
        Ta              constant 513.15;
        h               uniform 165;
        emissivityNozzle 0.95;
        emissivityFilament 0.95;
        dNozzle         2;
        dFilament       1.65;
        kappaMethod     fluidThermo;
        value           nonuniform 0();
    }
    wall_middle
    {
        type            fixedValue;
        value           uniform 513.15;
    }
    wall_bottom
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    procBoundary4to3
    {
        type            processor;
        value           uniform 405.65;
    }
    procBoundary4to5
    {
        type            processor;
        value           uniform 405.65;
    }
}


